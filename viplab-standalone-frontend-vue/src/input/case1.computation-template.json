{
  "identifier": "11483f23-95bf-424a-98a5-ee5868c85c3f",
  "version": "3.0.0",
  "metadata": {
    "displayName": "Fracture Benchmark (Case 1): Single Fracture",
    "description": "In this application, flow and transport through a single fracture in a cubic domain can be simulated with the numerical schemes available in Dumux, which comprise two cell-centered (tpfa, mpfa) and a vertex-centered finite volume scheme (box). Further details can be found in the related publication doi.org/10.1016/j.advwatres.2020.103759 .",
    "output": {
      "viewer": [
        "CSV",
        "ParaView"
      ],
      "ignore": ["results.csv"],
      "csv": [
        {
          "basename": "dataovertime",
          "xlabel": {
            "key": "time",
            "label": "Time in years",
            "factor": 3.1709792E-8,
            "format": ".2f"
          },
          "plots": [
            {
              "key": "massBottomLayer",
              "label": "Tracer mass in bottom layer in kg",
              "factor": 100
            },
            {
              "key": "massFracture",
              "label": "Tracer mass in fracture in kg"
            },
            {
              "key": "fluxAcrossOutlet",
              "label": "Flux across outlet in mg/(s m^2)",
              "factor": 1E6,
              "format": ".2f"
            }
          ]
        }
      ],
      "vtk": [
        {
          "basename": "case1_single_tracer_matrix"
        },
        {
          "basename": "case1_single_tracer_fracture"
        },
        {
          "basename": "case1_single_onep_matrix"
        },
        {
          "basename": "case1_single_onep_fracture"
        }
      ]
    }
  },
  "environment": "Container",
  "files": [
    {
      "identifier": "22483f42-95bf-984a-98a5-ee9485c85c3f",
      "path": "params.input",
      "metadata": {
        "syntaxHighlighting": "ini",
        "description": "params.input is the main configuration file for DuMux"
      },
      "parts": [
        {
          "identifier": "f3fc4404-3529-4962-b252-47bc4ddd02a2",
          "access": "template",
          "metadata": {
            "description": "Use the form fields to adjust a subset of parameters defined in 'params.input' of DuMux"
          },
          "parameters": [
            {
              "mode": "fixed",
              "identifier": "Discretization",
              "metadata": {
                "guiType": "dropdown",
                "name": "Discretization Method",
                "description": "Choose the discretization method. You can choose between three different finite volume (FV) methods implemented in DuMux."
              },
              "options": [
                {
                  "value": "tpfa",
                  "text": "TPFA (Cell Centered FV Scheme with Two-Point Flux Approximation)",
                  "selected": true
                },
                {
                  "value": "mpfa",
                  "text": "MPFA-O (Cell-Centered FV Scheme with Multi-Point Flux Approximation)"
                },
                {
                  "value": "box",
                  "text": "BOX (Vertex-Centered FV Scheme)"
                }
              ],
              "validation": "oneof"
            },
            {
              "mode": "fixed",
              "identifier": "Transport",
              "metadata": {
                "guiType": "radio",
                "name": "Solved Model Equations",
                "description": "Choose if the simulation stops after computing a stationary flow field, or if a subsequent transport simulation is carried out."
              },
              "options": [
                {
                  "value": "false",
                  "text": "Only Flow"
                },
                {
                  "value": "true",
                  "text": "Flow and Transport",
                  "selected": true
                }
              ],
              "validation": "oneof"
            },
            {
              "mode": "fixed",
              "identifier": "MeshFile",
              "metadata": {
                "guiType": "dropdown",
                "name": "Computational Grid",
                "description": "Select the total number of cells in the computational grid: coarse means ~1k cells, intermediate ~10k cells and fine ~100k cells."
              },
              "options": [
                {
                  "value": "single_1k.msh",
                  "text": "Coarse (1k Cells)",
                  "selected": true
                },
                {
                  "value": "single_10k.msh",
                  "text": "Intermediate (10k Cells)"
                },
                {
                  "value": "single_100k.msh",
                  "text": "Fine (100k Cells)"
                }
              ],
              "validation": "oneof"
            }
          ],
          "content": ""
        }
      ]
    }
  ],
  "configuration": {
